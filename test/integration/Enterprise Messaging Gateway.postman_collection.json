						{
							"listen": "test",
							"script": {
								"id": "9df718b2-767a-4641-94a2-28291f85fe3e",
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"var schema =  pm.variables.get(\"task_schema_detail\");",
									"",
									"pm.test('Schema is valid', function() {",
									"  var result=tv4.validateResult(JSON.parse(responseBody), schema);",
									"",
									"  if(!result.valid){",
									"        console.log(result);",
									"    }",
									"    ",
									"pm.expect(result.valid).to.be.true;",
									"",
									"});",
									"",
									"",
									"var jsResponse = JSON.parse(responseBody);",
									"",
									"postman.setEnvironmentVariable(\"task_id\", jsResponse.Id);",
									"",
									"",
									"pm.test(\"Content is valid\", function () {",
									"    var jsonData = pm.response.json();",
									"    console.log(jsonData.TaskParameters[0].Key);",
									"    pm.expect(jsonData.TaskParameters[0].Key).to.eql(\"username\");",
									"    pm.expect(jsonData.TaskParameters[1].Key).to.eql(\"timeout\");",
									"});",
									"",
									"tests[\"Task Parameter is valid\"] = jsResponse.TaskParameters[0].Key === \"username\";",
									"",
									""
								],
								"type": "text/javascript"
							}
